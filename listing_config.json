{
  "paypal": {
    "url": "https://paypal.eightfold.ai/api/apply/v2/jobs",
    "query": {
      "domain": ["paypal.com"],
      "location": "United States",
      "query": "Engineering",
      "start": "0",
      "num": "10",
      "sort_by": "relevance"
    },
    "keys": {
      "positions_key": "positions",
      "pos_id_key": "id",
      "pos_title_key": "name"
    }
  },
  "visa": {
    "method": "POST",
    "url": "https://search.visa.com/CAREERS/careers/jobs?q=",
    "payload": {
      "filters": [{ "superDepartment": ["Technology", "Data Science"] }],
      "city": [
        "Ashburn",
        "Atlanta",
        "Austin",
        "Bellevue",
        "Denver",
        "Foster City",
        "Highlands Ranch",
        "Los Angeles",
        "Mentor",
        "Miami",
        "San Francisco",
        "Toronto",
        "Washington",
        "Wilmington"
      ],
      "from": 0,
      "size": 10
    },
    "keys": {
      "positions_key": "jobDetails",
      "pos_id_key": "refNumber",
      "pos_title_key": "jobTitle"
    }
  }
}
